# SSH client configuration
# defines bookmarks/templates for hosts and options to quickly connect to known hosts

Host box
	HostName 46.101.206.216
	IdentityFile ~/.ssh/box.bugabinga.net.key

Host github.com
	HostName github.com
	User git
	IdentityFile ~/.ssh/github.key

Host seaduck
	Hostname seaduck
	User oli
	IdentityFile ~/.ssh/seaduck.key

Host artifactory
	Hostname artifactory.isp-insoft.de
	User java

# catch-all pattern
Host *
	User root
	Compression yes
	IdentitiesOnly yes
	AddKeysToAgent yes
