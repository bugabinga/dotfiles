{
	"$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
	"diagnostics": {
		"globals": [
			"vim"
		],
		"neededFileStatus": {
			"codestyle-check": "Any"
		}
	},
	"workspace": {
		"checkThirdParty": false
	},
	"runtime": {
		"version": "LuaJIT",
		"pathStrict": true,
		"special": {
			"want": "require"
		}
	}
}
